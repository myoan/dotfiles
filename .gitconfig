[user]
	name = myoan
	email = motoki.yoan@gmail.com
[color]
	ui = true
[alias]
	br = branch
	ci = commit
	co = checkout
	st = status
[core]
	excludesfile = ~/.gitconfig
	editor = vim
[editor]
    editor = vim
    pager = "nfs -s | LESSCHARSET=utf-8 less"
    autoCRLF = false

[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
[diff]
	compactionHeuristic = true

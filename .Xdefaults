bin/bash: X: 96
Xft.antialias: true
!Xft.antialias: false
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
 
!-------------------------------------------------------------------------------
! URxvt settings
! Colours lifted from Solarized (http://ethanschoonover.com/solarized)
! More info at:
! http://pod.tst.eu/http://cvs.schmorp.de/rxvt-unicode/doc/rxvt.1.pod
!-------------------------------------------------------------------------------
 
URxvt.depth: 32
URxvt.geometry: 90x30
URxvt.transparent: true
URxvt.fading: 0
! URxvt.urgentOnBell: true
! URxvt.visualBell: true
URxvt.loginShell: true
URxvt.saveLines: 50
URxvt.internalBorder: 3
URxvt.lineSpace: 0
 
! Fonts
URxvt.allow_bold: false
/* URxvt.font: -*-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-1 */
URxvt*font: xft:Migu 2m:style=Regular:size=11:antialias=true
URxvt*boldFont: xft:Migu 2m:style=Bold:size=11:antialias=true
!URxvt*font: xft:Mensch:pixelsize=13
!URxvt*boldFont: xft:Mensch:pixelsize=13
!URxvt*font: xft:Monospace:pixelsize=13
!URxvt*boldFont: xft:Monospace:pixelsize=13
 
! Fix font space
URxvt*letterSpace: -1
 
! Scrollbar
URxvt.scrollStyle: rxvt
URxvt.scrollBar: false
 
! Perl extensions
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1
URxvt.urlLauncher: firefox
 
! Cursor
URxvt.cursorBlink: true
URxvt.cursorColor: #657b83
URxvt.cursorUnderline: false
 
! Pointer
URxvt.pointerBlank: true

! Xresources file

URxvt*.transparent: true
!! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt*.shading: 15

!colors:
!scheme: pnevma
!*foreground: #696969
*foreground: #ffdccc
*background: #282626
!*foreground: #6F6767
!*background: #3A3636
*color0: #363636
*color8: #424242
*color1: #C37561
*color9: #D19485
*color2: #A0A57E
*color10: #B6B99D
*color3: #D1A375
*color11: #DEBC9C
*color4: #7985A3
*color12: #98A1B9
*color5: #AB716D
*color13: #BE918E
*color6: #98B9B1
*color14: #CBE6CB
*color7: #D0D0D0
*color15: #E6E6E6
